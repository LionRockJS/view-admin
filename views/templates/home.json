{
  "wrapper": "div.container.container-columns",
  "sections": {
    "signup": {
      "type": "card-signup",
      "settings": {
        "enabled": "{{allowSignup}}"
      }
    },
    "login": {
      "type": "card-login",
      "settings": {
        "enabled": "{% if allowSignup == false %}true{% endif %}"
      }
    }
  },
  "order": [
    "signup",
    "login"
  ]
}